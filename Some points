class Point:
	def __init__(self, x=0, y=0): #create instance
		self.x = x
		self.y = y
	
a = []
N = 0
while N < 5:
    x = float(input("Input number for x:"))
    y = float (input ("Input number for y: "))
    N+=1
    crd = Point(x,y)#instance
    a.append(crd)
j = 1
for i in a:
    print(f"Точка {j}:", end=" ") 
    for key, value in i.__dict__.items():
        print(str(key) + " = " + str(value), end= " ")
    print() # every result  - on new line
    j+=1
